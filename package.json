    "build": "gatsby build --verbose",
    "clean": "rm -rf ./.cache/* && rm -rf ./public/*",
    "deploy": "yarn run clean && git push origin source && yarn run build && cp CNAME public/ && gh-pages -b gh-pages -d public",
